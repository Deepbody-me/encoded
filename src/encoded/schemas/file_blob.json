{
    "title": "File blob",
    "description": "Schema for reporting a unique instance of S3 blob.",
    "id": "/profiles/file_blob.json",
    "$schema": "http://json-schema.org/draft-04/schema#",
    "type": "object",
    "required": ["blob_url"],
    "additionalProperties": false,
    "identifyingProperties": ["uuid"],
    "mixinProperties": [
        { "$ref": "mixins.json#/schema_version" },
        { "$ref": "mixins.json#/uuid" },
        { "$ref": "mixins.json#/notes" },
        { "$ref": "mixins.json#/standard_status" }
    ],
    "properties": {
        "schema_version": {
            "default": "1"
        },
        "blob_url": {
            "title": "Blob path-style URL",
            "description": "Path-style URL of a file stored at S3 bucket.",
            "comment": "Unique path-style URL of a file stored at S3 bucket.",
            "type": "string",
            "pattern": "^s3://encode-files.*$"
        }
    },
    "facets": {
        "status": {
            "title": "Status"
        }
    },
    "changelog": "/profiles/changelogs/file_blob.md"
}